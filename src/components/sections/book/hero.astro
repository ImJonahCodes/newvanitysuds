<!-- Defining a grid container that holds all the content -->
<section class="mx-auto h-full w-full gap-4 px-4 py-14 sm:px-6 md:items-center md:gap-8 lg:px-8 2xl:max-w-full">
  <iframe id="myIframe" src="https://app.urable.com/virtual-shop/FyMeQRTSA0XU11LDiEBQ" style="width: 100%; border: none;"></iframe>
</section>

<script>
  // Function to set the iframe height to the viewport height
  function setIframeHeight() {
    var iframe = document.getElementById('myIframe');
    iframe.style.height = window.innerHeight + 'px';
  }

  // Set initial height
  setIframeHeight();

  // Update height when the window is resized
  window.addEventListener('resize', setIframeHeight);
</script>

